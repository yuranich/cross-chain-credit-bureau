{
    "env": {
        "node": true
    },
    "parser": "@typescript-eslint/parser",
    "extends": ["plugin:@typescript-eslint/recommended", "plugin:prettier/recommended"],
    "rules": {
        "@typescript-eslint/no-unused-vars": ["off"],
        "@typescript-eslint/no-explicit-any": ["off"],
        "prettier/prettier": [
            "warn",
            {
                "endOfLine": "auto"
            }
        ]
    }
}
